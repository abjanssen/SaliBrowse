        }
      },
      "displays": [
        {
          "type": "LinearBasicDisplay",
          "displayId": "HSISS4_sorted.gff-LinearBasicDisplay",
          "mouseover": "jexl:italics(get(feature,'name')) || get(feature,'locus_tag')",
          "renderer": {
            "type": "SvgFeatureRenderer",
            "color1": "jexl:get(feature,'type')=='tRNA'?'rgb(45, 41, 138)': get(feature,'type')=='tmRNA'?'rgb(45, 41, 138)': get(feature,'type')=='rRNA'?'rgb(136, 184, 77)' : get(feature,'type')=='mRNA_region'?'grey':'rgb(215, 163, 32)'",
            "labels": {
              "name": "jexl:get(feature,'name') || get(feature,'locus_tag')"
            }
          }
        },
        {
          "type": "LinearArcDisplay",
          "displayId": "HSISS4_sorted.gff-LinearArcDisplay"
        }
      ],
      "formatDetails": {
        "feature": "jexl:{name:italics(feature.name), type:replaceuc(feature.type), Source:replaceuc(feature.source), source:undefined, phase: undefined, id:undefined, Gene:italics(feature.gene), gene:undefined, Locus_tag:feature.locus_tag, locus_tag:undefined, New_locus_tag:feature.new_locus_tag, new_locus_tag:undefined, Old_locus_tag:feature.old_locus_tag, old_locus_tag:undefined, UniProt: feature.uniprot != null ? '<a href=https://www.uniprot.org/uniprotkb/'+feature.uniprot+'>'+feature.uniprot+'</a>' : null, AlphaFold: feature.uniprot != null ? '<a href=https://alphafold.ebi.ac.uk/entry/'+feature.uniprot+'>'+'AlphaFold'+'</a>' : null, Foldseek: feature.uniprot != null ? '<a href=https://search.foldseek.com/search?accession='+feature.uniprot+'&source=AlphaFoldDB>'+'Foldseek'+'</a>' : null,  NCBI:feature.protein_id != null ? '<a href=https://www.ncbi.nlm.nih.gov/protein/'+feature.protein_id+'>'+'NCBI'+'</a>' : null, protein_id:undefined, uniprot:undefined, STRING: feature.stringdb != null ? '<a href=https://string-db.org/cgi/network?identifiers='+feature.stringdb+'>'+'STRING'+'</a>' : null, stringdb:undefined}",
        "subfeatures": "jexl:{accession:undefined, name:italics(feature.name), type:capital(feature.type), Source:replaceuc(feature.source), source:undefined, phase:undefined, id:undefined, parent:undefined, EC_number: feature.ec_number != null ? '<a href=https://enzyme.expasy.org/EC/'+feature.ec_number+'>'+feature.ec_number+'</a>' : null , ec_number:undefined, Gene:italics(feature.gene), COG_number: feature.db_xref != null ? '<a href=https://www.ncbi.nlm.nih.gov/research/cog/cog/'+removecog(feature.db_xref)+'>'+removecog(feature.db_xref)+'</a>' : null, db_xref:undefined, gene:undefined, Inference:feature.inference != null ? capital(inference(feature.inference)) : null, inference:undefined, locus_tag:undefined, Product:capital(feature.product), product:undefined, protein_id:undefined, RFAM: feature.accession != null ? '<a href=https://rfam.org/family/'+feature.accession+'>'+feature.accession+'</a>' : null, note:undefined, Note: feature.note != null ? removequotes(feature.note): null}"
      }
    },
